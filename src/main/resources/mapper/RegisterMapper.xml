<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.todolist.mapper.RegisterMapper">
    <insert id="insertMember" parameterType="com.example.todolist.domain.MemberVO">
        INSERT INTO member (login_id, login_passwd, name)
        VALUES (#{login_id}, #{login_passwd}, #{name})
    </insert>

</mapper>

